@model Equipe
@{
    ViewData["Title"] = "Equipes";
}
<form method="POST" action='@Url.Action("Cadastrar")' enctype="multipart/form-data">
    <label>ID</label>
    <input type="text" name="IdEquipe" />

    <label>Nome</label>
    <input type="text" name="Nome" />

    <label>Imagem</label>
    <input type="file" accept="image/*" name="Imagem" />


    <button type="submit">Cadastrar</button>
</form>

<table class="table table-striped table-responsive">
    <thead>
        <th>ID</th>
        <th>Nome</th>
        <th>Logo</th>
    </thead>
    <tbody>
        @foreach (Equipe item in ViewBag.Equipes)
        {
            <tr>
                <td>@item.IDEquipe</td>
                <td>@item.Nome</td>
                <td>
                    <img style="width: 25px;" src="/images/Equipes/@item.Imagem" alt="Logo do time @item.Nome" />
                </td>
                <td>
                    <a class="navbar-brand" asp-area="" asp-controller="Equipe" asp-action="Deletar" asp-route-id="@item.IDEquipe">Excluir</a>
                </td>
            </tr>
        }
    </tbody>
</table>